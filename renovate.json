{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "branchConcurrentLimit": 20,
  "enabledManagers": ["pub"],
  "packageRules": [
    {
      "matchManagers": ["pub"]
    },
    {
      "matchPackagePrefixes": [
        "^adaptive_app/.*"
      ],
      "groupName": "adaptive_app"
    }
  ]
}
